// 3|hello|false|3|3|true|testing|another string

import "err"

let a = 3
err.println(a)

let b = "hello"
err.println(b)

let c = false
err.println(c)

let d = a
err.println(d)

b = a
err.println(b)

d = true
err.println(d)

let word = "testing"
err.println(word)

word = "another string"
err.println(word)
